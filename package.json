{
  "name": "song-list-linca-tojou",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:linca": "cross-env NODE_ENV=production SITE_ID=linca NEXT_PUBLIC_BASE_PATH=/song-list-linca-tojou next build",
    "build:reshra": "cross-env NODE_ENV=production SITE_ID=reshra NEXT_PUBLIC_BASE_PATH=/song-list-reshra-hanamaki next build",
    "start": "next start",
    "lint": "next lint",
    "fetch:json": "powershell -Command \"Invoke-WebRequest -Uri 'http://localhost:3000/api/generate' -OutFile 'public/songs.json'\"",
    "deploy": "npm run build && gh-pages -d out",
    "generate:csv": "npx ts-node -P tsconfig.scripts.json scripts/generateCsv.ts",
    "generate:json": "npx ts-node -P tsconfig.scripts.json scripts/generateJson.ts",
    "test": "jest"
  },
  "dependencies": {
    "@next/third-parties": "^16.0.1",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "axios": "^1.8.1",
    "dotenv": "^16.4.7",
    "lucide-react": "^0.477.0",
    "next": "15.1.9",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.6.0",
    "papaparse": "^5.5.2",
    "playwright": "^1.50.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "gh-pages": "^6.3.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
